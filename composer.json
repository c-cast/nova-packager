{
    "name": "c-cast/nova-packager",
    "description": "A package to modularize Laravel Nova 4",
    "type": "project",
    "keywords": [
        "laravel",
        "nova",
        "modules",
        "packages"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/nova": "^4.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../nova"
        }
    ],
    "autoload": {
        "psr-4": {
            "CCast\\NovaPackager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CCast\\NovaPackager\\NovaPackagerServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "cCast",
            "email": "info@ccast.it"
        }
    ],
    "minimum-stability": "dev"
}
